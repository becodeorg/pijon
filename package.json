{
  "name": "@becode/pijon",
  "version": "1.0.5",
  "description": "Pijon, the delivery man - File upload lambda preparator",
  "private": true,
  "scripts": {
    "lint": "npx eslint --fix --cache src",
    "build": "npx babel src --out-dir bin",
    "work": "npx babel src -w --out-dir bin",
    "deploy:dev": "npx sls deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/becodeorg/pijon.git"
  },
  "author": "leny <leny@becode.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/becodeorg/pijon/issues"
  },
  "homepage": "https://github.com/becodeorg/pijon#readme",
  "lint-staged": {
    "*.yml": [
      "npx prettier --write",
      "git add"
    ],
    "*.json": [
      "npx prettier --write",
      "git add"
    ],
    "*.js": [
      "npx eslint --fix --cache",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@becode/eslint-config": "^0.9.1",
    "@sentry/cli": "^1.52.1",
    "aws-sdk": "^2.657.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-unicorn": "^18.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "serverless": "^1.67.3",
    "serverless-domain-manager": "^3.3.1",
    "serverless-offline": "^6.1.4",
    "serverless-s3-local": "^0.5.4"
  },
  "dependencies": {
    "@sentry/node": "^5.15.4",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa2-cors": "^2.0.6",
    "serverless-http": "^2.3.2",
    "uuid": "^7.0.3"
  }
}
