{
  "name": "@becode/pijon",
  "version": "1.0.6",
  "description": "Pijon, the delivery man - File upload lambda preparator",
  "private": true,
  "scripts": {
    "lint": "npx eslint --fix --cache src",
    "build": "npx babel src --out-dir bin",
    "work": "npx babel src -w --out-dir bin",
    "push:release": "sh ./scripts/push-release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/becodeorg/pijon.git"
  },
  "author": "leny <leny@becode.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/becodeorg/pijon/issues"
  },
  "homepage": "https://github.com/becodeorg/pijon#readme",
  "lint-staged": {
    "*.yml": [
      "npx prettier --write",
      "git add"
    ],
    "*.json": [
      "npx prettier --write",
      "git add"
    ],
    "*.js": [
      "npx eslint --fix --cache",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@becode/eslint-config": "^0.10.0",
    "@sentry/cli": "^1.54.0",
    "aws-sdk": "^2.695.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-unicorn": "^19.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "serverless": "^1.72.0",
    "serverless-domain-manager": "^4.1.1",
    "serverless-offline": "^6.4.0",
    "serverless-s3-local": "^0.6.1"
  },
  "dependencies": {
    "@sentry/node": "^5.17.0",
    "koa": "^2.12.1",
    "koa-body": "^4.2.0",
    "koa2-cors": "^2.0.6",
    "serverless-http": "^2.5.0",
    "uuid": "^8.1.0"
  }
}
