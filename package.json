{
  "name": "@becode/pijon",
  "version": "1.0.6",
  "description": "Pijon, the delivery man - File upload lambda preparator",
  "private": true,
  "scripts": {
    "lint": "npx eslint --fix --cache src",
    "build": "npx babel src --out-dir bin",
    "work": "npx babel src -w --out-dir bin",
    "push:release": "sh ./scripts/push-release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/becodeorg/pijon.git"
  },
  "author": "leny <leny@becode.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/becodeorg/pijon/issues"
  },
  "homepage": "https://github.com/becodeorg/pijon#readme",
  "lint-staged": {
    "*.yml": [
      "npx prettier --write",
      "git add"
    ],
    "*.json": [
      "npx prettier --write",
      "git add"
    ],
    "*.js": [
      "npx eslint --fix --cache",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@becode/eslint-config": "^0.8.2",
    "@sentry/cli": "^1.51.1",
    "aws-sdk": "^2.613.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-unicorn": "^16.1.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1",
    "serverless": "^1.64.0",
    "serverless-domain-manager": "^3.3.1",
    "serverless-offline": "^5.12.1",
    "serverless-s3-local": "^0.5.4"
  },
  "dependencies": {
    "@sentry/node": "^5.12.4",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa2-cors": "^2.0.6",
    "serverless-http": "^2.3.1",
    "uuid": "^7.0.0"
  }
}
